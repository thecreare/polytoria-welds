{
  generator: "readable", // or "dense" "readable" "retain_lines"
  bundle: {
    require_mode: {
      name: "path",
      module_folder_name: "init",
      sources: {
        src: "./src",
      },
    },
  },
  rules: [
    "compute_expression",
    "convert_luau_number",
    "convert_square_root_call", // Possible optimization, converts to ^
    // "remove_assertions",
    "remove_continue",
    "remove_compound_assignment",
    // "remove_debug_profiling",
    // "remove_floor_division", // May be unsupported, todo: check
    "remove_if_expression",
    "remove_interpolated_string",
    "remove_types",
  
    // Created by previous rules sometimes
    "remove_empty_do",
    "filter_after_early_return",
  ],
}
